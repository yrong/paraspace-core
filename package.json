{
  "name": "@paraspace/core-v1",
  "version": "1.1.0",
  "description": "ParaSpace Protocol V1 core smart contracts",
  "files": [
    "contracts",
    "artifacts",
    "types"
  ],
  "scripts": {
    "prepare": "husky install",
    "size": "hardhat size-contracts",
    "build": "SKIP_LOAD=true hardhat compile",
    "lint": "tsc --noEmit -skipLibCheck && solhint 'contracts/**/*.sol' && prettier --check 'contracts/**/*.sol' 'deploy/**/*.ts' 'test-suites/**/*.ts' && eslint . --ext .ts",
    "coverage": "TS_NODE_TRANSPILE_ONLY=1 hardhat coverage --testfiles 'test-suites/*.ts'",
    "format": "prettier --write 'contracts/**/*.sol' 'deploy/**/*.ts' 'test-suites/**/*.ts'",
    "doc": "hardhat docgen",
    "test": "TS_NODE_TRANSPILE_ONLY=1 hardhat test ./test-suites/*.ts",
    "clean": "hardhat clean && rm -rf ./artifacts ./cache ./types"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@looksrare/sdk": "^0.7.4",
    "@nomiclabs/hardhat-ethers": "^2.0.0",
    "@nomiclabs/hardhat-etherscan": "^3.1.0",
    "@nomiclabs/hardhat-waffle": "2.0.3",
    "@openzeppelin/contracts": "4.2.0",
    "@openzeppelin/contracts-upgradeable": "4.2.0",
    "@openzeppelin/hardhat-upgrades": "^1.20.0",
    "@tenderly/hardhat-tenderly": "1.1.0-beta.5",
    "@typechain/ethers-v5": "^10.1.0",
    "@typechain/hardhat": "^6.1.2",
    "@types/chai": "^4.2.11",
    "@types/lowdb": "1.0.9",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "5.33.0",
    "@typescript-eslint/parser": "5.33.0",
    "@uniswap/v3-periphery": "1.4.1",
    "@uniswap/v3-sdk": "^3.9.0",
    "@x2y2-io/sdk": "^0.1.4",
    "chai": "4.2.0",
    "chai-bignumber": "3.0.0",
    "defender-relay-client": "^1.7.0",
    "dotenv": "^16.0.1",
    "eslint": "^8.22.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eth-sig-util": "2.5.3",
    "ethereum-waffle": "3.2.0",
    "ethereumjs-util": "7.0.2",
    "ethers": "^5.0.19",
    "hardhat": "^2.6.4",
    "hardhat-contract-sizer": "^2.0.3",
    "hardhat-gas-reporter": "^1.0.0",
    "hardhat-typechain": "^0.3.3",
    "husky": "^8.0.1",
    "lowdb": "1.0.0",
    "prettier": "^2.4.1",
    "prettier-plugin-solidity": "^1.0.0-alpha.53",
    "pretty-quick": "^3.1.1",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.8.0-rc.test.0",
    "solidity-docgen": "^0.6.0-beta.26",
    "ts-generator": "^0.1.1",
    "ts-node": "^10.9.1",
    "typechain": "^8.1.0",
    "typescript": "4.7.4"
  },
  "dependencies": {
    "async": "^3.2.4",
    "hardhat-deploy": "^0.11.4",
    "tmp-promise": "^3.0.2"
  },
  "author": "ParaSpace-MM Team",
  "contributors": [],
  "license": "MIT",
  "keywords": [
    "paraspace",
    "nft",
    "marketplace",
    "money-market",
    "protocol",
    "protocol-v1",
    "ethereum",
    "solidity"
  ],
  "publishConfig": {
    "registry": "https://npm.pkg.github.com"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/para-space/paraspace-core"
  }
}
